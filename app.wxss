/**app.wxss**/
page{
  background-color: #eee;
  height: 100%;

}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}


.cls_commBtn {
  display: flex;
  font-size: 40rpx;
  font-weight: bold;
  color: #48c13d;
  margin: 0 40rpx;
  flex-direction: column;
  margin-top: 60rpx;
  align-items: center;
}

.cls_commEditLeftLabe {
  display: block;
  height: 60rpx;
  font-size: 38rpx;
  color: #48c13d;
  margin: 0 0 0 40rpx;
  flex-direction: row;
  align-items: center;
}

.cls_commEdit {
  display: flex;
  height: 2rem;
  font-size: 38rpx;
  color: #48c13d;
  margin: 0 40rpx;
}

